{
  "expo": {
    "name": "JobHunter AI Pro",
    "slug": "jobhunter",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "jobhunter",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1E3A5F"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mounemus.jobhunteraipro",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "JobHunter AI Pro utilise la caméra pour scanner vos documents CV.",
        "NSPhotoLibraryUsageDescription": "JobHunter AI Pro accède à vos photos pour télécharger votre CV et photo de profil.",
        "NSFaceIDUsageDescription": "JobHunter AI Pro utilise Face ID pour sécuriser l'accès à vos données personnelles.",
        "NSDocumentPickerUsageDescription": "JobHunter AI Pro accède à vos fichiers pour télécharger votre CV.",
        "CFBundleLocalizations": ["fr", "en"],
        "CFBundleDevelopmentRegion": "fr"
      },
      "config": {
        "usesNonExemptEncryption": false
      },
      "privacyManifests": {
        "NSPrivacyAccessedAPITypes": [
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryUserDefaults",
            "NSPrivacyAccessedAPITypeReasons": ["CA92.1"]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1E3A5F"
      },
      "package": "com.mounemus.jobhunteraipro",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "USE_BIOMETRIC",
        "USE_FINGERPRINT",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-local-authentication",
      "expo-notifications",
      "expo-document-picker"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "87b0ccbb-a467-4639-82d9-1b232e4d298d"
      }
    },
    "owner": "mounemus",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/87b0ccbb-a467-4639-82d9-1b232e4d298d"
    }
  }
}
